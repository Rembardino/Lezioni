    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "t=np.array(range(10))\n",
    "\n",
    "\n",
    "s0=10  #posizione iniziale al tempo 0\n",
    "v=4 #velocit√° \n",
    "s=s0+v*t\n",
    "\n",
    "plt.figure()\n",
    "plt.xlim(0,s0+v*max(t))\n",
    "plt.xlabel(\"Spazio [m]\")\n",
    "plt.ylabel(\"Tempo [s]\")\n",
    "plt.plot(s,t)\n",
    "plt.show()\n"
   ]
